# moving the turtle
# right now the forward and backward funcs also print turtle location for testing purposes
def moveFd(tur):
    tur.forward(5)
    print(tur.pos())

def moveBkwd(tur):
    tur.backward(2.5)
    print(tur.pos())

def turnLeft(tur):
    tur.left(45)

def turnRight(tur):
    tur.right(45)
